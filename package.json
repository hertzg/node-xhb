{
  "name": "xhb",
  "version": "1.1.0",
  "description": "Provides ability to read and modify xhb files used by HomeBank",
  "main": "build/index.js",
  "author": {
    "name": "George Kotchlamazashvili <georgedot@gmail.com>",
    "email": "georgedot@gmail.com"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "xhb",
    "file",
    "parser",
    "parse",
    "serialize",
    "serializer",
    "read",
    "reader",
    "write",
    "writer",
    "home",
    "bank",
    "homebank",
    "ts",
    "typescript"
  ],
  "license": "LGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/hertzg/node-xhb.git"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "type-check": "tsc -p tsconfig.build.json --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly --listEmittedFiles",
    "build:js": "babel src --out-dir build --extensions \".ts,.tsx\" --ignore **/__tests__ --verbose",
    "prettier": "prettier",
    "test": "jest",
    "release": "semantic-release"
  },
  "dependencies": {
    "printj": "^1.2.2",
    "xml-parser": "^1.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^27.0.0",
    "@types/node": "^16.0.0",
    "@types/xml-parser": "^1.2.29",
    "jest": "^27.0.6",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3",
    "semantic-release": "^18.0.0",
    "conventional-changelog-conventionalcommits": "^4.5.0",
    "@semantic-release/commit-analyzer": "^9.0.0",
    "@semantic-release/release-notes-generator": "^10.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/exec": "^6.0.0",
    "@semantic-release/npm": "^8.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^8.0.0",
    "prettier": "^2.2.1"
  }
}
